	quest.427B9C6D95885DE2.quest_subtitle: "Manzana Red Delicious + Azalea Florecida"
	quest.427C3AFC0FF131CD.title: "Fluidos"
	quest.4281BCDCE233F65B.quest_desc: ["Estos pueden ser encontrados en cofres dentro de Ciudades Antiguas."]
